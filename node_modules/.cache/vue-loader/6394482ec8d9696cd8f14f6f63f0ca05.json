\" checked>\n                    <label for=\"male\" class=\"label-sex\">男</label>\n                    <input type=\"radio\" name=\"sex\" id=\"female\" value=\"1\">\n                    <label for=\"female\" class=\"label-sex\">女</label>\n                </div>\n                <div>\n                    <label for=\"sNo\">学号</label><input type=\"text\" id=\"sNo\" name=\"sNo\">\n                </div>\n                <div>\n                    <label for=\"email\">邮箱</label><input type=\"text\" id=\"email\" name=\"email\">\n                </div>\n                <div>\n                    <label for=\"birth\">出生年</label><input type=\"text\" id=\"birth\" name=\"birth\">\n                </div>\n                <div>\n                    <label for=\"phone\">手机号</label><input type=\"text\" id=\"phone\" name=\"phone\">\n                </div>\n                <div>\n                    <label for=\"address\">住址</label><input type=\"text\" id=\"address\" name=\"address\">\n                </div>\n                <div>\n                    <label for=\"\"></label>\n                    <input type=\"submit\" class=\"btn\" id=\"edit-submit-btn\">\n                </div>\n            </form>\n        </div>\n        </transition>\n    </div>\n</template>\n\n<script>\nimport { mapMutations } from 'vuex'\nexport default {\n    methods : {\n        ...mapMutations(['setShowModal'])\n    }\n}\n</script>\n\n<style scoped>\n.modal-leave-to,\n.modal-enter {\n    /* transform: translateY(-100%); */\n    top:0%;\n    opacity:0;\n}\n.modal-enter-active,\n.modal-leave-active {\n    transition:all 1s;\n}\n.modal-leave,\n.modal-enter-to {\n    /* transform: translateY(0); */\n    top:50%;\n    opacity:1;\n}\n</style>"]}]}